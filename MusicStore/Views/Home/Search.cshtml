@using MusicStore.Models
@{
    ViewBag.Title = "Search Results";
    if (Model.Count == 0)
    {
            <p>No Results Found</p>
    }
    else
    {
        // GRADY - We're not using 'Album' as the model for this view; we're
        // using AlbumListModel.  Change 'Album' to 'var'.
        foreach (Album a in Model)
        {
            string s = a.AlbumId + ".jpg";
            <div class="row searchResult">
                <div class="col-md-2">
                    <img src="@(s)"/>
                    <div class="col-md-10">
                        <h2>@(a.Title)</h2>
                        <p>@(a.Artist), @(a.Year)</p>
                    </div>
                </div>
            </div>
        }
    }
}
<h2>@ViewBag.Title.</h2>
